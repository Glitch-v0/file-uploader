<div>
  <h2>Folders</h2>
  <div class="folder-container">
    <% folders.forEach(folder => { %>
    <div class="folder-card">
      <a href="/cloud/<%= folder.id %>">
        <img class="icon folder" src="/icons/folder.svg" alt="folder icon">
      </a>
      <a href="/cloud/<%= folder.id %>">
        <h3><%= folder.name %></h3> 
      </a>     
      <a href="/cloud/<%= folder.id %>/deleteFolder">
        <img class="icon delete" src="/icons/delete.svg" alt="delete icon" />
      </a>
      <a href="/cloud/<%= folder.id %>/updateFolder">
        <img class="icon update" src="/icons/pencil.svg" alt="folder update">
      </a>
    </div>
    <% }) %>
  </div>
</div>
